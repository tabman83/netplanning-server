/*jshint onevar: false, indent:4 */
/*global setImmediate: false, setTimeout: false, console: false */
(function () {

    var ScheduleItem = function(regexMatch) {
    	this.begin 		= new Date(parseInt(regexMatch[1],10));
    	this.end 		= new Date(parseInt(regexMatch[2],10));
    	this.kind 		= regexMatch[3];
    	this.id 		= regexMatch[4] || null;
    	this.name		= regexMatch[5] || null;
    	this.comment 	= regexMatch[6] || null; 
    }

    // Node.js
    if (typeof module !== 'undefined' && module.exports) {
        module.exports = ScheduleItem;
    }
    // AMD / RequireJS
    else if (typeof define !== 'undefined' && define.amd) {
        define([], function () {
            return ScheduleItem;
        });
    }
    // included directly via <script> tag
    else {
        this.ScheduleItem = ScheduleItem;
    }

}());


